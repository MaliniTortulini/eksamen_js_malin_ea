{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ES2020", 
    "jsx": "react-jsx", 
    "esModuleInterop": true,
    "strict": true,
    "moduleResolution": "node",
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true
  },
  "include": ["src"],
  "exclude": ["node_modules"]
}

